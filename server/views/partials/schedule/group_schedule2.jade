.container
    .well
        fieldset
            legend {{groupName}} Schedules - Period from {{period.startDate | date : format : timezone}} to {{period.endDate | date : format : timezone}}
            .calendar(ui-calendar="uiConfig.calendar" ng-model="eventSources")
            .loading(id="loading")
                img.loading-image(id="loading-image" src="/images/load-image.gif")
            .col-md-5.col-md-offset-5
                a.btn.btn-default.printButtonClass(href="javascript: window.print()") Print
                | &nbsp
                a.btn.btn-default.printButtonClass(href="/schedule/groups") Back to Groups

